package entities;

public class _005_Account {

	public String number;
	public String name;
	public char option;
	public double initial;
	public double deposit;
	public double withDraw;
	public double saldo;
	public char y;
	
	public _005_Account(String number, String name, char option, double initial) {
		this.number = number;
		this.name = name;
		this.option = option;
		char y;
		if (this.option = y) {
			this.initial = initial;	
		}
		
	}
	
	public _005_Account(double deposit, double withDraw) {
		this.deposit = deposit;
		this.withDraw = withDraw;
	}
	
	
	public String toString() {
		saldo = saldo + deposit - withDraw;
		return String.format("%.2f", saldo) ;
	}
		

}
